{
    "extends": [
        "next/core-web-vitals",
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error"
    },
    "overrides": [
        {
            "files": ["**/*.test.js", "**/__tests__/**"],
            "env": {
                "jest": true
            }
        }
    ]
}
